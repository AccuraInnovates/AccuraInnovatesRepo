@model Accura_Innovatives.EmployeeViewModel


@{
    ViewData["Title"] = "Edit";
}
@*      @{
                                            var defaultValue = "@Html.DisplayFor(model => model.Gender)"; // This could be set based on your logic

} *@

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>

        html, body {
            margin: 0;
            padding: 0;
            font-family: Arial, sans-serif;
            /* background: url('../Image/Welding.jpg') center/cover no-repeat fixed; */
            height: 100%;
            background-attachment: fixed;
        }

        body {
            display: flex;
            align-items: center;
            min-height: 100vh;
            text-align: left;
        }

        .menu-item {
            display: inline-block;
            padding: 2px;
            border: 1px solid #ccc;
            margin: 5px;
        }

        #profile-image {
            max-width: 20%;
            height: auto;
            margin-bottom: 16px;
            display: none; /* Initially hide the image */
        }

        #selectedList {
            list-style-type: none;
            padding: 0;
        }

        .deleteButton {
            cursor: pointer;
            color: blue;
            margin-left: 10px;
        }

        form {
            display: flex;
            flex-direction: column;
            align-items: center;
            top: 65px;
            left: 12.5%;
            background-color: rgba(255, 255, 255, 0.8);
            position: absolute;
            box-sizing: border-box;
            border: 1px solid #ccc;
            width: 80%;
        }

        /* Optional: Add some styling to improve readability */
        label {
            display: block;
            margin-bottom: 8px;
            font-weight: bold;
        }

        input, select, textarea {
            width: 100%;
            padding: 8px;
            margin-bottom: 16px;
            box-sizing: border-box;
        }

        /* Style for the tabs */
        .tabs {
            padding-top: 2px;
            padding-bottom: 2px;
            display: flex;
            justify-content: center;
            /* margin-bottom: 14px; */
            text-align: center;
            height: 30px;
        }

        .tab:active {
            background-color: deepskyblue;
        }

        .tab {
            padding-top: 2px;
            padding-bottom: 2px;
            cursor: pointer;
            border: 1px solid lightblue;
            border-radius: 4px;
            margin: 0 4px;
            background-color: lightblue;
            font-size: 12px;
            text-align: center;
        }

        /* Hide all tab content by default */
        .tab-content {
            display: none;
            font-size: 14px;
            padding: 5px;
            /* text-align: center; */
        }

        .centered-text {
            text-align: center; /* Center-align text */
            font-size: 14px;
        }
        /* Show the selected tab content */
        .tab-content.active {
            display: block;
        }

        .heads {
            text-decoration: underline;
            font-size: 15px;
        }

        .certificatesDocsVerifiedConditional-fields, .aadharCardVerifiedConditional-fields, .salConditional-fields, .conditional-fields, .addPreviousCompany0Conditional-fields, .addPreviousCompany1Conditional-fields, .addPreviousCompany2Conditional-fields, .addPreviousCompany3Conditional-fields {
            display: none;
        }

        .formXIEligibilityConditional-fields, .acknowledgementGivenConditional-fields, .eligibilityForAssetConditional-fields, .salaryOrWagesConditional-fields {
            display: none;
        }

        .reassignSameEmpIdConditional-fields, .otherAttachment4Conditional-fields, .otherAttachment3Conditional-fields, .otherAttachment2Conditional-fields, .otherAttachment1Conditional-fields {
            display: none;
        }

        .addServiceBreak1Conditional-fields, .addServiceBreak2Conditional-fields, .addServiceBreak3Conditional-fields, .attachLeaveReleavedConditional-fields, .otherCertificates1Conditional-fields, .otherCertificates2Conditional-fields, .attachCertificate4Conditional-fields, .attachCertificate3Conditional-fields, .attachCertificate2Conditional-fields, .attachCertificate1Conditional-fields {
            display: none;
        }

        .tab:active {
            background-color: deepskyblue;
        }

        .Edit-Container {
            top: 65px;
            left: 12.5%;
            background-color: rgba(255, 255, 255, 0.8);
            position: absolute;
            box-sizing: border-box;
            text-size-adjust: 12px;
            border: 1px solid #ccc;
            width: 80%;
        }

        h1, h4 {
            text-shadow: 2px 2px 3px black;
        }

    </style>
</head>
<body>


    <form class="Edit-Container" asp-action="Edit" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <h1>Edit</h1>

        <h4>Employee Master Data</h4>



        <div class="tabs centered-text">
            <div class="tab" onclick="showTab('tab1')">Personal Details</div>
            <div class="tab" onclick="showTab('tab2')">Family Details</div>
            <div class="tab" onclick="showTab('tab3')">Education Details</div>
            <div class="tab" onclick="showTab('tab4')">Pre.Professional Details</div>
            <div class="tab" onclick="showTab('tab5')">New Joinee Details</div>
            <div class="tab" onclick="showTab('tab6')">Verification</div>

            <!-- Add more tabs as needed -->
        </div>

        @if (!string.IsNullOrEmpty(ViewBag.ErrorMessage))
        {
            <p style="color: red;">@ViewBag.ErrorMessage</p>
        }
        <hr />


        <input type="hidden" asp-for="EmpCtg" />
        <input type="hidden" asp-for="EmpCode" />

        <ng-form enctype="multipart/form-data">
            <!-- Personal Details Tab -->
            <div id="tab1" class="tab-content active">
                <div style="border:black 1px solid; padding: 10px;">
                    <label class="heads" for="EmpDetails">Employee Details:</label>

                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="EmpName" class="control-label">Employee Name*:</label>
                                <input asp-for="EmpName" class="form-control" required />
                                <span asp-validation-for="EmpName" class="text-danger"></span>
                            </div>

                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PerEmail" class="control-label">Personal Mail Id:</label>
                                <input asp-for="PerEmail" class="form-control" />
                                <span asp-validation-for="PerEmail" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PerMobile" class="control-label">Personal Phone No*:</label>
                                <input asp-for="PerMobile" class="form-control" required />
                                <span asp-validation-for="PerMobile" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">



                                <label asp-for="Gender" class="control-label">Gender*:</label>
                                <input asp-for="Gender" class="form-control" required>
                                <span asp-validation-for="Gender" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="BloodGrp" class="control-label">Blood Group:</label>
                                <select asp-for="BloodGrp" class="form-control" asp-items="ViewBag.BloodGrp"></select>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="MaritalStatus" class="control-label">Marital Status:</label>
                                <input asp-for="MaritalStatus" class="form-control" />
                                <span asp-validation-for="MaritalStatus" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="ProfilePhoto" class="control-label">Upload Photo:</label>
                                <input asp-for="ProfilePhoto" type="file" id="imageInput" class="form-control" />
                                <span asp-validation-for="ProfilePhoto" class="text-danger"></span>
                                <div id="uploaded-image-container">
                                    <div id="imagePreview"></div>
                                </div>
                                <label asp-for="ProfilePhoto" class="control-label">Previously added:</label>
                                @if (Model != null && Model.ProfilePhoto != null)
                                {

                                    @if (Model.ProfilePhoto != null && Model.ProfilePhoto.FileName != null && Model.ProfilePhoto.FileName != "" && Model.ProfilePhoto.FileName != "NULL" && Model.ProfilePhoto.FileName.Length > 0)
                                    {
                                        @if (Model.ProfilePhoto != null && Model.ProfilePhoto.FileName.EndsWith(".jpg", StringComparison.OrdinalIgnoreCase) || Model.ProfilePhoto.FileName.EndsWith(".jpeg", StringComparison.OrdinalIgnoreCase) || Model.ProfilePhoto.FileName.EndsWith(".png", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <img src="~/images/@Model.ProfilePhoto.FileName" width="200px" height="200px" />
                                        }
                                        else if (Model.ProfilePhoto != null && Model.ProfilePhoto.FileName.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <embed src="~/images/@Model.ProfilePhoto.FileName" width="200px" height="200px" type="application/pdf" />
                                        }
                                        else
                                        {
                                            <span>No File Provided</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>No File Provided</span>
                                    }
                                }
                                else
                                {
                                    <span>No File Provided</span>
                                }


                            </div>
                        </div>
                    </div>
                </div>

                <br />

                <div style="border:black 1px solid; padding: 10px;">
                    <label class="heads" for="EmpAadharDetails">Employee Aadhar Details:</label>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="AadharNo" class="control-label">Aadhar No.*:</label>
                                <input type="number" asp-for="AadharNo" class="form-control" required />
                                <span asp-validation-for="AadharNo" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="EmpAadharName" class="control-label">Aadhar Name*:</label>
                                <input asp-for="EmpAadharName" class="form-control" required />
                                <span asp-validation-for="EmpAadharName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="AadharDob" class="control-label">Aadhar DOB*:</label>
                                <input asp-for="AadharDob" type="date" class="form-control" required />
                                <span asp-validation-for="AadharDob" class="text-danger"></span>
                                <label asp-for="AadharDob" class="control-label">Previously selected:</label>
                                <input type="text" readonly value="@Html.DisplayFor(model => model.AadharDob)" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="Dob" class="control-label">DOB:</label>
                                <input asp-for="Dob" type="date" class="form-control" />
                                <span asp-validation-for="Dob" class="text-danger"></span>
                                <label asp-for="Dob" class="control-label">Previously selected:</label>
                                <input asp-for="Dob" type="text" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="AadharCard" class="control-label">Upload Aadhar Photo:</label>
                                <input asp-for="AadharCard" type="file" id="aadharImageInput" class="form-control" />
                                <span asp-validation-for="AadharCard" class="text-danger"></span>
                                <div class="col-md-4" id="uploaded-image-container">
                                    <div id="aadharImagePreview"></div>
                                </div>
                                <label asp-for="AadharCard" class="control-label">Previously selected:</label>
                                @if (Model != null && Model.AadharCard != null)
                                {
                                    @if (Model.AadharCard != null && Model.AadharCard.FileName != null && Model.AadharCard.FileName != "" && Model.AadharCard.FileName != "NULL" && Model.AadharCard.FileName.Length > 0)
                                    {
                                        @if (Model.AadharCard != null && Model.AadharCard.FileName.EndsWith(".jpg", StringComparison.OrdinalIgnoreCase) || Model.AadharCard.FileName.EndsWith(".jpeg", StringComparison.OrdinalIgnoreCase) || Model.AadharCard.FileName.EndsWith(".png", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <img src="~/images/@Model.AadharCard.FileName" width="200px" height="200px" />
                                        }
                                        else if (Model.AadharCard != null && Model.AadharCard.FileName.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <embed src="~/images/@Model.AadharCard.FileName" width="200px" height="200px" type="application/pdf" />
                                        }
                                        else
                                        {
                                            <span>No File Provided</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>No File Provided</span>
                                    }
                                }
                                else
                                {
                                    <span>No File Provided</span>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div style="border:black 1px solid; padding: 10px;">
                    <label class="heads" for="EmpPanDetails">Employee Pancard Details:</label>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PanNo" class="control-label">Pan No.:</label>
                                <input asp-for="PanNo" class="form-control" />
                                <span asp-validation-for="PanNo" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="EmpPanName" class="control-label">Pan Name*:</label>
                                <input asp-for="EmpPanName" class="form-control" required />
                                <span asp-validation-for="EmpPanName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PanCard" class="control-label">Upload PanCard:</label>
                                <input asp-for="PanCard" type="file" id="panImageInput" class="form-control" />
                                <span asp-validation-for="PanCard" class="text-danger"></span>
                                <div class="col-md-4" id="uploaded-image-container">
                                    <div id="panImagePreview"></div>
                                </div>
                                <label asp-for="PanCard" class="control-label">Previously added:</label>
                                @if (Model != null && Model.PanCard != null)
                                {
                                    @if (Model.PanCard != null && Model.PanCard.FileName != null && Model.PanCard.FileName != "" && Model.PanCard.FileName != "NULL" && Model.PanCard.FileName.Length > 0)
                                    {
                                        @if (Model.PanCard != null && Model.PanCard.FileName.EndsWith(".jpg", StringComparison.OrdinalIgnoreCase) || Model.PanCard.FileName.EndsWith(".jpeg", StringComparison.OrdinalIgnoreCase) || Model.PanCard.FileName.EndsWith(".png", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <img src="~/images/@Model.PanCard.FileName" width="200px" height="200px" />
                                        }
                                        else if (Model.PanCard != null && Model.PanCard.FileName.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <embed src="~/images/@Model.PanCard.FileName" width="200px" height="200px" type="application/pdf" />
                                        }
                                        else
                                        {
                                            <span>No File Provided</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>No File Provided</span>
                                    }
                                }
                                else
                                {
                                    <span>No File Provided</span>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div style="border:black 1px solid; padding: 10px;">
                    <label class="heads" for="EmpNameDetails">Employee Name Details & Nationality:</label>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="EmpCertifName" class="control-label">Employee Name in Education Certificate</label>
                                <br />
                                <input asp-for="EmpCertifName" class="form-control" />
                                <span asp-validation-for="EmpCertifName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="EmpBcardName" class="control-label">Preferred Name in Business card & AWRKPL records</label>
                                <input asp-for="EmpBcardName" class="form-control" />
                                <span asp-validation-for="EmpBcardName" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="Nationality" class="control-label">Nationality</label>
                                <br />
                                <select asp-for="Nationality" class="form-control" asp-items="ViewBag.Nationality"></select>
                            </div>
                        </div>


                    </div>
                </div>
                <br />

                <div style="border:black 1px solid; padding: 5px;">
                    <label class="heads" for="EmpDrvLinDetails">Employee Driving Licence Details:</label>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="DrvLinNo" class="control-label">Driving Licence No.:</label>
                                <input asp-for="DrvLinNo" class="form-control" />
                                <span asp-validation-for="DrvLinNo" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="DrvLinVal" class="control-label">Driving Licence Validity:</label>
                                <input asp-for="DrvLinVal" type="date" class="form-control" />
                                <span asp-validation-for="DrvLinVal" class="text-danger"></span>
                                <label asp-for="DrvLinVal" class="control-label">Previously selected:</label>
                                <input asp-for="DrvLinVal" type="text" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="DrvLinCard" class="control-label">Upload Driving Licence Photo:</label>
                                <input asp-for="DrvLinCard" type="file" class="form-control" id="drivingLicenceImageInput" />
                                <span asp-validation-for="DrvLinCard" class="text-danger"></span>
                                <div class="col-md-4" id="uploaded-image-container">
                                    <div id="drivingLicenceImagePreview"></div>
                                </div>
                                <label asp-for="DrvLinCard" class="control-label">Previously added:</label>
                                @if (Model != null && Model.DrvLinCard != null)
                                {
                                    @if (Model.DrvLinCard != null && Model.DrvLinCard.FileName != null && Model.DrvLinCard.FileName != "" && Model.DrvLinCard.FileName != "NULL" && Model.DrvLinCard.FileName.Length > 0)
                                    {
                                        @if (Model.DrvLinCard != null && Model.DrvLinCard.FileName.EndsWith(".jpg", StringComparison.OrdinalIgnoreCase) || Model.DrvLinCard.FileName.EndsWith(".jpeg", StringComparison.OrdinalIgnoreCase) || Model.DrvLinCard.FileName.EndsWith(".png", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <img src="~/images/@Model.DrvLinCard.FileName" width="200px" height="200px" />
                                        }
                                        else if (Model.DrvLinCard != null && Model.DrvLinCard.FileName.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <embed src="~/images/@Model.DrvLinCard.FileName" width="200px" height="200px" type="application/pdf" />
                                        }
                                        else
                                        {
                                            <span>No File Provided</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>No File Provided</span>
                                    }
                                }
                                else
                                {
                                    <span>No File Provided</span>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <div style="border:black 1px solid; padding: 5px;">
                    <label class="heads" for="EmpPassportDetails">Employee Passport Details:</label>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PassportNo" class="control-label">Passport No.:</label>
                                <input asp-for="PassportNo" class="form-control" />
                                <span asp-validation-for="PassportNo" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PassportVal" class="control-label">Passport Validity:</label>
                                <input asp-for="PassportVal" type="date" class="form-control" />
                                <span asp-validation-for="PassportVal" class="text-danger"></span>
                                <label asp-for="PassportVal" class="control-label">Previously selected:</label>
                                <input asp-for="PassportVal" type="text" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PassportCard" class="control-label">Upload Passport Photo:</label>
                                <input asp-for="PassportCard" type="file" class="form-control" id="passportImageInput" />
                                <span asp-validation-for="PassportCard" class="text-danger"></span>
                                <div class="col-md-4" id="uploaded-image-container">
                                    <div id="passportImagePreview"></div>
                                </div>
                                <label asp-for="PassportCard" class="control-label">Previously added:</label>
                                @if (Model != null && Model.PassportCard != null)
                                {
                                    @if (Model.PassportCard != null && Model.PassportCard.FileName != null && Model.PassportCard.FileName != "" && Model.PassportCard.FileName != "NULL" && Model.PassportCard.FileName.Length > 0)
                                    {
                                        @if (Model.PassportCard != null && Model.PassportCard.FileName.EndsWith(".jpg", StringComparison.OrdinalIgnoreCase) || Model.PassportCard.FileName.EndsWith(".jpeg", StringComparison.OrdinalIgnoreCase) || Model.PassportCard.FileName.EndsWith(".png", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <img src="~/images/@Model.PassportCard.FileName" width="200px" height="200px" />
                                        }
                                        else if (Model.PassportCard != null && Model.PassportCard.FileName.EndsWith(".pdf", StringComparison.OrdinalIgnoreCase))
                                        {
                                            <embed src="~/images/@Model.PassportCard.FileName" width="200px" height="200px" type="application/pdf" />
                                        }
                                        else
                                        {
                                            <span>No File Provided</span>
                                        }
                                    }
                                    else
                                    {
                                        <span>No File Provided</span>
                                    }
                                }
                                else
                                {
                                    <span>No File Provided</span>
                                }
                            </div>
                        </div>

                    </div>
                </div>
                <br />
                <div style="border:black 1px solid; padding: 5px;">
                    <label class="heads" for="presentAddress">Present Address:</label>
                    <div class="row">

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="PresentAddressLine1" class="control-label">Address Line 1.*:</label>
                                <input asp-for="PresentAddressLine1" class="form-control" required />
                                <span asp-validation-for="PresentAddressLine1" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="PresentAddressLine2" class="control-label">Address Line 2.:</label>
                                <input asp-for="PresentAddressLine2" class="form-control" />
                                <span asp-validation-for="PresentAddressLine2" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PresentAddressCity" class="control-label">City*:</label>
                                <input asp-for="PresentAddressCity" class="form-control" required />
                                <span asp-validation-for="PresentAddressCity" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PresentAddressState" class="control-label">State*:</label>
                                <input asp-for="PresentAddressState" class="form-control" required />
                                <span asp-validation-for="PresentAddressState" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PresentAddressZipcode" class="control-label">Zip/Postal Code*:</label>
                                <input asp-for="PresentAddressZipcode" class="form-control" required />
                                <span asp-validation-for="PresentAddressZipcode" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
